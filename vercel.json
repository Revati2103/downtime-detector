{
    "version": 2,
    "builds": [
      {
        "src": "client/next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
   
  }
  